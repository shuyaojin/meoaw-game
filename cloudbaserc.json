{
  "envId": "your-env-id",
  "framework": {
    "name": "meoaw-game",
    "plugins": {}
  },
  "functions": [
    {
      "name": "steam-sync",
      "config": {
        "timeout": 60,
        "envVariables": {
          "SYNC_BATCH_SIZE": "5",
          "SYNC_DELAY_MS": "1500"
        },
        "runtime": "Nodejs16.13",
        "memorySize": 256
      },
      "handler": "index.main",
      "ignore": [
        "node_modules",
        "node_modules/**/*"
      ]
    },
    {
      "name": "steam-api",
      "config": {
        "timeout": 10,
        "envVariables": {},
        "runtime": "Nodejs16.13",
        "memorySize": 256
      },
      "handler": "index.main",
      "ignore": [
        "node_modules",
        "node_modules/**/*"
      ]
    }
  ],
  "region": "ap-shanghai"
}
